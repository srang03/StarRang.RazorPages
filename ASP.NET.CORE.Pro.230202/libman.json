{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.6.3",
      "destination": "wwwroot/lib",
      "files": [ "jquery.js", "jquery.min.js" ]
    },
    {
      "library": "jquery-validate@1.19.5",
      "destination": "wwwroot/lib",
      "files": [ "additional-methods.js", "additional-methods.min.js", "jquery.validate.js", "jquery.validate.min.js" ]
    },
    {
      "library": "jquery-validation-unobtrusive@4.0.0",
      "destination": "wwwroot/lib",
      "files": [ "jquery.validate.unobtrusive.js", "jquery.validate.unobtrusive.min.js" ]
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@5.2.3",
      "destination": "wwwroot/lib/bs",
      "files": ["dist/js/bootstrap.js", "dist/js/bootstrap.min.js"]
    }
  ]
}